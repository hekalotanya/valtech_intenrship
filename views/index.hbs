                  {{> post__icon}}
                  <span class="infrormation__name">by ANNA BRONES</span>
                </div>
            </div>
          </div>
          <div class="post_block__arrows">
              <div class="post_block__prev arrow"></div>
              <div class="post_block__next arrow"></div>
          </div>
        </div>
        <a href="#" class="news__view_all view_all">View all posts</a>
      </div>
    </section>
    <section class="main__subscribe subscribe">
      <span class="subscribe__sign">Subscribe Newletter</span>
      <h2 class="subscribe__title">Sign up for weekly newsletter</h2>
      <form class="subscribe__form form">
        <input type="email" name="email" class="form__input input" placeholder="Enter your email" required>
        <button class="form__submit submit">
          {{> submit_icon}}
        </button>
      </form>
      <div class="subscribe__icons">
        <a class="icon__link" href="#">
          {{> icon_facebook}}
        </a>
        <a class="icon__link" href="#">
          {{> icon_twitter}}
        </a>
        <a class="icon__link" href="#">
          {{> icon_fit}}
        </a>
        <a  class="icon__link"href="#">
          {{> icon_skype}}
        </a>       
      </div>
    </section>
  </main>
  <footer class="page__footer footer" id="contact">
    <div class="footer__contacts contacts">
      <h2 class="contacts__title">Orga Fresh</h2>
      <div class="contacts__time">
        <span class="time">Monday – Saturday: 8:00 am – 4:00pm</span>
        <span class="time">Sunday: 9:00 am – 5:00pm</span>
      </div>
      <div class="contacts__adress">
        <span class="adress">29 Rue du Bourg Tibourg,</span>
        <span class="adress">75004 Paris, France</span>
      </div>
      <a class="contacts__mail" href="mailto: Organicfood@gmail.com">Organicfood@gmail.com</a>
    </div>
    <div class="footer__menu menu">
