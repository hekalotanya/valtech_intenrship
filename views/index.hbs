      <div class="about__quotes_block quotes_block">
        <div class="quotes_block__quote quote quote--visible">
          <div class="quote__header">
            <div class="header__image"></div>
            <div class="header__information information">
              <span class="information__name">Sonya Sahni</span>
              <span class="information__country">Gurugram, Haryana</span>
            </div>
          </div>
